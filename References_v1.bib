	Pages = {346-51},
	Pmid = {9878540},
	Pst = {ppublish},
	Title = {{Phosphatidylinositol 3-kinase in HL-60 nuclei is bound to the nuclear matrix and increases during granulocytic differentiation}},
	Volume = {253},
