	Journal = {Proc Natl Acad Sci U S A},
	Keywords = {Amino Acid Sequence; Base Sequence; Carrier Proteins; Cloning, Molecular; Gene Expression Regulation; Genes; Humans; Molecular Sequence Data; Multigene Family; Receptors, Retinoic Acid; Sequence Homology, Nucleic Acid; Transcription, Genetic; Tretinoin},
