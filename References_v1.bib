	Doi = {10.1186/1752-0509-5-180},
	Journal = {BMC Syst Biol},
	Journal-Full = {BMC systems biology},
	Mesh = {Adipocytes; Alanine; Computer Simulation; Diabetes Mellitus; Genome, Human; Hepatocytes; Humans; Metabolic Networks and Pathways; Metabolomics; Models, Biological; Muscle Cells; Obesity; Systems Biology},
	Pages = {180},
	Pmc = {PMC3219569},
