	Pmid = {26474788},
	Pst = {ppublish},
	Title = {iSCHRUNK--In Silico Approach to Characterization and Reduction of Uncertainty in the Kinetic Models of Genome-scale Metabolic Networks},
	Volume = {33},
	Year = {2016},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.ymben.2015.10.002}}

@article{Khanin:1989aa,
	Abstract = {Linear mathematical models of the kinetics of blood coagulation have previously been presented (Levine, 1966, Science, N.Y. 152, 651; Martorana & Moro, 1974, Math. Biosci. 21, 77). In this paper a non-linear mathematical model of the extrinsic pathway of blood coagulation is presented to take into account a positive feedback. The feedback is due to factor Va as a co-factor involved in thrombin formation. The extrinsic pathway is shown to function as an amplifier cascade if a vessel wall injury exceeds a threshold value. For sub-threshold stimulation, the extrinsic pathway does not function.},
