	Pmid = {18206540},
	Timestamp = {2008.03.05},
	Title = {Basal level micronucleus frequency in stimulated lymphocytes of untreated patients with leukemia.},
	Url = {http://dx.doi.org/10.1016/j.cancergencyto.2007.10.009},
	Volume = {180},
	Year = {2008},
