	Pii = {12/9/2730},
	Pmid = {16675564},
	Timestamp = {2009.06.25},
	Title = {Expression and nuclear localization of ErbB3 in prostate cancer.},
	Url = {http://dx.doi.org/10.1158/1078-0432.CCR-05-2242},
	Volume = {12},
	Year = {2006},
