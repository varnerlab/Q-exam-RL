	Journal = {J Trauma},
	Journal-Full = {The Journal of trauma},
	Mesh = {Accidents, Traffic; Adolescent; Adult; Aged; Aged, 80 and over; Cause of Death; Child; Child, Preschool; Colorado; Cross-Sectional Studies; Female; Humans; Infant; Male; Middle Aged; Retrospective Studies; Wounds and Injuries; Wounds, Penetrating},
	Month = {Feb},
