	Owner = {Ryan Tasseff},
	Pages = {14477--14482},
	Pii = {0606836103},
	Pmid = {16973750},
	Timestamp = {2008.09.03},
	Title = {Combinatorial activities of Akt and B-Raf/Erk signaling in a mouse model of androgen-independent prostate cancer.},
