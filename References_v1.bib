	Month = {Jan},
	Number = {2},
	Owner = {rat44},
	Pages = {179--184},
	Pii = {S0960-9822(06)02579-6},
	Pmid = {17174095},
	Timestamp = {2011.04.15},
	Title = {CK2 Is a component of the KSR1 scaffold complex that contributes to Raf kinase activation.},
