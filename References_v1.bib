	Journal = {Nat Med},
	Keywords = {Androgens; Animals; Humans; Male; Mice; Mice, SCID; Neoplasm Metastasis; Neoplasm Staging; Neoplasm Transplantation; Prostatic Neoplasms; Transplantation, Heterologous},
