	Pages = {229-48},
	Pmid = {10716907},
	Pst = {ppublish},
	Title = {Theory for the systemic definition of metabolic pathways and their use in interpreting metabolic function from a pathway-oriented perspective},
	Volume = {203},
	Year = {2000},
