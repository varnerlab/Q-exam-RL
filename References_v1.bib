	Owner = {Ryan Tasseff},
	Pages = {1522--1535},
	Pmid = {12006650},
	Timestamp = {2007.09.27},
	Title = {Coordinated traffic of Grb2 and Ras during epidermal growth factor receptor endocytosis visualized in living cells.},
