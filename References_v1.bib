	Pages = {719--732},
	Pii = {S0092-8674(06)00502-2},
	Pmid = {16713564},
	Timestamp = {2009.10.22},
	Title = {Downregulation of PP2A(Cdc55) phosphatase by separase initiates mitotic exit in budding yeast.},
	Url = {http://dx.doi.org/10.1016/j.cell.2006.03.038},
	Volume = {125},
	Year = {2006},
