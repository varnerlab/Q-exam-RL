	Doi = {10.1073/pnas.1202582110},
	Journal = {Proc Natl Acad Sci U S A},
	Journal-Full = {Proceedings of the National Academy of Sciences of the United States of America},
	Mesh = {Escherichia coli K12; Feedback, Physiological; Fructose-Bisphosphatase; Genes, Bacterial; Glycolysis; Kinetics; Metabolic Networks and Pathways; Models, Biological; Pyruvate Kinase; Transcription, Genetic},
	Month = {Jan},
	Number = {3},
	Pages = {1130-5},
	Pmc = {PMC3549114},
	Pmid = {23277571},
	Pst = {ppublish},
	Title = {Functioning of a metabolic flux sensor in Escherichia coli},
