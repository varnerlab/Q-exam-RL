	Keywords = {modeling; kinetics; linlog; power law; systems biology; parameter identificiation; identifiability;},
	Month = {March},
	Number = {2},
	Pages = {87--100},
	Publisher = {ACADEMIC PRESS INC ELSEVIER SCIENCE},
	Times-Cited = {16},
	Title = {Translating biochemical network models between different kinetic formats},
