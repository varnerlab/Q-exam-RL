	Pmc = {PMC4128477},
	Pmid = {24958246},
	Pst = {ppublish},
	Title = {Kinetic model facilitates analysis of fibrin generation and its modulation by clotting factors: implications for hemostasis-enhancing therapies},
	Volume = {10},
